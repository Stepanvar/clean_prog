<Window x:Class="PatternTutorial.Duck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PatternTutorial"
        mc:Ignorable="d"
        Title="Duck" Height="450" Width="800">
    <Grid>
		<TextBlock x:Name="duckinst" VerticalAlignment="Center" HorizontalAlignment="Center" Width="200"/>
		<WrapPanel>
			<ComboBox x:Name="classes" Margin="5" MinWidth="200" IsReadOnly="True" DropDownClosed="classes_DropDownClosed" ></ComboBox>
			<ComboBox x:Name="quack" Margin="5" MinWidth="200" IsReadOnly="True" DropDownClosed="quack_DropDownClosed"/>
		</WrapPanel>
	</Grid>
</Window>
